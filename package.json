{
  "name": "mh-wilds-event-scraper",
  "version": "1.0.4",
  "description": "A scraper for Monster Hunter Wilds event quests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "example": "ts-node -r tsconfig-paths/register example/parse-event.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "monster hunter",
    "scraper",
    "mh wilds"
  ],
  "author": "imtheme2206",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtheme2206/mh-wilds-event-scraper.git"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.3",
    "axios": "^1.9.0",
    "jsdom": "^26.1.0"
  }
}
